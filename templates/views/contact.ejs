<%- include('../partials/head.ejs') %>

    <%- include('../partials/navbar.ejs') %>

        <h1 class="display-4">CONTACT</h1>
        <form method="post" action="/send">
            <div class="row">
                <div class="form-group col-md-6  mb-4">
                    <label for="formName">Name</label>
                    <input type="text" class="form-control shadow border-0" id="formName" name="name" placeholder="name"
                        value="aaa">
                </div>
                <div class="form-group col-md-6 mb-4">
                    <label for="formMail">Email address</label>
                    <input type="email" class="form-control shadow border-0" id="formMail" name="email"
                        placeholder="user@mail.com" value="as@as.com">
                </div>
            </div>
            <div class="form-group mb-4">
                <label for="formMessage">Message</label>
                <textarea class="form-control shadow-lg border-0" name="message" id="formMessage" rows="3"
                    placeholder="Message">hehe</textarea>
            </div>
            <button type="submit" class="btn btn-dark mb-2">Submit</button>
        </form>

